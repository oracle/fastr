JAVAC=$(JAVAHOME)/bin/javac
JAVAH=$(JAVAHOME)/bin/javah
JAR=$(JAVAHOME)/bin/jar
JRIDEPS=win32/libjvm.dll.a
JNIPREFIX=
PLATFORMT=run.bat
JRILIB=jri.dll
